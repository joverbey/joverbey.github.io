#define ITERATIONS 5

int main() {
	/* How long does setBuf take? */
	printf("setBuf: ");
	for (int i = 0; i < ITERATIONS; i++) {
		double start = omp_get_wtime();
		setBuf();
		printf(" %f", omp_get_wtime() - start);
	}
	printf(" seconds\n");

	/* How long does memset take? */
	printf("memset: ");
